import java.util.*;
public class Wheel{
  
  private int upper;
  private Random random;
  
  public Wheel(int seed, int upper){
    this.upper=upper;
    random = new Random(seed);
  }
  
  public int spin(){
    return random.nextInt(upper)+1;
  }
  
  /*
   * TESTING
   *
    public static void main(String[] args){
    Wheel myWheel = new Wheel(133,11);
    for(int i = 0; i< 20; i++){
      System.out.print(myWheel.spin() + "  ");
    }
  }*/
  
}